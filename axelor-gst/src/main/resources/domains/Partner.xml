<domain-models xmlns="http://axelor.com/xml/ns/domain-models" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.1.xsd">

  <module name="base" package="com.axelor.apps.base.db" />

  <entity name="Partner" lang="java">

    <!-- Type of the partner's contact details -->
    <string name="contactType" selection="axelor.gst.partner.contact.type.select" title="type" />
    <!-- Adding secondary email -->
    <one-to-one name="secEmailAddress" ref="com.axelor.apps.message.db.EmailAddress" title="Sec Email" />
    <string name="gstin" max="15" min="15" unique="true"/>
  </entity>
</domain-models>
  